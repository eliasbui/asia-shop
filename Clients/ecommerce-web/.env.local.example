# API URLs
NEXT_PUBLIC_PRODUCT_API_URL=http://localhost:8080
NEXT_PUBLIC_USER_API_URL=https://localhost:7031
NEXT_PUBLIC_STORAGE_API_URL=http://localhost:8080

# Authentication Web URL (separate auth application)
NEXT_PUBLIC_AUTH_WEB_URL=http://localhost:3001

# Cookie Configuration (for cross-domain auth)
NEXT_PUBLIC_COOKIE_DOMAIN=localhost
NEXT_PUBLIC_COOKIE_SECURE=false

# Feature flags
NEXT_PUBLIC_USE_MOCK_API=false
NEXT_PUBLIC_ENABLE_MSW=true

# Google reCAPTCHA
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your-recaptcha-site-key

# Google Analytics
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Environment
NEXT_PUBLIC_ENV=development
